        <contexts>
            <context name="Plain" attribute="Plain" lineEndContext="#pop" >
                <keyword String="etl-blocks" attribute="EtlBlock" context="#stay" />
                <keyword String="flow-control" attribute="FlowControl" context="#stay" />
                <keyword String="inputs" attribute="Plugin" context="#stay" />
                <keyword String="filters" attribute="Plugin" context="#stay" />
                <keyword String="outputs" attribute="Plugin" context="#stay" />
                <keyword String="booleans" attribute="Boolean" context="#stay" />
                <Float attribute="Float" context="#stay" />
                <Int attribute="Decimal" context="#stay" />
                <StringDetect String=" => [" attribute="Plain" context="Conf" />
                <StringDetect String=" code => '" attribute="Plain" context="Ruby" />
                <!--<StringDetect String=" code => &quot;" attribute="Plain" context="Ruby" />-->
                <DetectChar char="&quot;" attribute="String" context="StringDQ" />
                <DetectChar char="'" attribute="String" context="StringSQ" />
                <DetectChar char="#" attribute="Comment" context="Comment" />
                <DetectChar char="[" attribute="Plain" context="Field" />
                <RegExpr String=" \/" attribute="Regexp" context="Regexp" />
            </context>
            <context name="StringDQ" attribute="String" lineEndContext="#stay" >
                <RegExpr String="(?:%{\[|%{|\[)" attribute="Plain" context="Field" />
                <RegExpr String="[^\\]?&quot;" attribute="String" context="#pop" />
            </context>
            <context name="StringSQ" attribute="String" lineEndContext="#stay" >
                <RegExpr String="(?:%{\[|%{|\[)" attribute="Plain" context="Field" />
                <RegExpr String="[^\\]?'" attribute="String" context="#pop" />
            </context>
            <context name="Comment" attribute="Comment" lineEndContext="#pop">
                <!--<IncludeRules context="##Comments"/>-->
            </context>
            <context name="Field" attribute="Field" lineEndContext="#stay" >
                <DetectChar char="," attribute="Plain" context="#stay" />
                <DetectChar char="&quot;" attribute="String" context="StringDQ" />
                <DetectChar char="'" attribute="String" context="StringSQ" />
                <RegExpr String="(?:]}|]|})" attribute="Plain" context="#pop" />
            </context>
            <context name="Regexp" attribute="Regexp" lineEndContext="#stay" >
                <RegExpr String="[^\\]/ " attribute="Regexp" context="#pop" />
            </context>
            <context name="Conf" attribute="Plain" lineEndContext="#stay" >
                <DetectChar char="&quot;" attribute="String" context="StringDQ" />
                <DetectChar char="'" attribute="String" context="StringSQ" />
                <RegExpr String="]" attribute="Plain" context="#pop" />
            </context>
            <context name="Ruby" attribute="Plain" lineEndContext="#stay" >
                <IncludeRules context="##Ruby"/>
                <RegExpr String="[^\\]'" attribute="Plain" context="#pop" />
                <!--<RegExpr String="[^\\]&quot;" attribute="Plain" context="#pop" />-->
            </context>
        </contexts>

        <itemDatas>
            <itemData name="Plain"       defStyleNum="dsNormal" />
            <itemData name="Boolean"     defStyleNum="dsKeyword" />
            <itemData name="Field"       defStyleNum="dsAttribute" />
            <itemData name="String"      defStyleNum="dsString" />
            <itemData name="Float"       defStyleNum="dsFloat"/>
            <itemData name="Decimal"     defStyleNum="dsDecVal"/>
            <itemData name="EtlBlock"    defStyleNum="dsKeyword" />
            <itemData name="FlowControl" defStyleNum="dsControlFlow" />
            <itemData name="Plugin"      defStyleNum="dsConstant" />
            <itemData name="Comment"     defStyleNum="dsComment"/>
            <itemData name="Regexp"      defStyleNum="dsExtension"/>
        </itemDatas>

